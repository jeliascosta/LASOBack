"12100200.xhp\n"
"par_id3153360\n"
"16\n"
"help.text"
msgid "<ahelp hid=\"cui/ui/fmsearchdialog/rbSingleField\">Searches through a specified data field.</ahelp>"
msgstr "<ahelp hid=\"cui/ui/fmsearchdialog/rbSingleField\">Badantino data bare giddoonni hasanno.</ahelp>"

