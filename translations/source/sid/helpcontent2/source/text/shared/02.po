msgstr "<image id=\"img_id3147226\" src=\"cmd/sc_sbanativesql.png\"><alt id=\"alt_id3147226\">Bido</alt></image>"

#: 14030000.xhp
msgctxt ""
"14030000.xhp\n"
"par_id3155893\n"
"4\n"
"help.text"
msgid "Run SQL command directly"
msgstr "SQL Hajajo  hattegedeenni ha'risanno."

